{
  "name": "chess-player",
  "version": "0.3.0",
  "description": "In-browser chess game and simulation built with React.",
  "repository": "git@github.com:findawayer/chess-player.git",
  "homepage": "https://chess-player.vercel.app/",
  "author": "Tae Sung, Lim <findawayer@gmail.com>",
  "license": "ISC",
  "keywords": [
    "chess",
    "fun",
    "react-app",
    "typescript"
  ],
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "next dev",
    "server": "node dist/server",
    "server:clean": "rm -rf dist",
    "server:build": "npm -s run clean && npm -s run generate && tsc",
    "server:dev": "ts-node-dev --no-notify --respawn --transpile-only src/server",
    "database": "prisma studio",
    "database:validate": "prisma introspect",
    "generate": "prisma generate && ts-node --transpile-only src/database",
    "migrate:commit": "prisma migrate dev --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:push": "prisma migrate deploy --preview-feature",
    "format": "prettier --write .",
    "lint": "eslint --fix --ext .ts,.tsx,.js .",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@prisma/client": "^2.13.1",
    "@reduxjs/toolkit": "^1.5.0",
    "apollo-server-express": "^2.19.1",
    "bcrypt": "^5.0.0",
    "chess.js": "^0.11.0",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "immutability-helper": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "microframework": "^0.6.4",
    "next": "^10.0.3",
    "nexus": "1.0.0",
    "nexus-plugin-prisma": "^0.27.0",
    "nodemailer": "^6.4.17",
    "nprogress": "^0.2.0",
    "pretty-ms": "^7.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-resize-detector": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.7",
    "@types/chess.js": "^0.10.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.16",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-resize-detector": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-import": "^1.13.3",
    "cypress": "^6.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "module-alias": "^2.2.2",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma"
  }
}
