{
  "name": "chess-player",
  "version": "0.3.0",
  "description": "In-browser chess game and simulation built with React.",
  "repository": "git@github.com:findawayer/chess-player.git",
  "homepage": "https://chess-player.vercel.app/",
  "author": "Tae Sung, Lim <findawayer@gmail.com>",
  "license": "ISC",
  "keywords": [
    "chess",
    "fun",
    "typescript",
    "react",
    "nextjs",
    "graphql",
    "prisma",
    "postgresql",
    "nexus"
  ],
  "scripts": {
    "start": "concurrently \"yarn server:start\" \"yarn client:start\"",
    "dev": "concurrently \"yarn server:dev\" \"yarn client:dev\"",
    "build": "yarn schema:generate && next build",
    "client:start": "next start",
    "client:dev": "next dev",
    "server:start": "nodemon --ext ts,graphql --exec ts-node -- --files ./src/server",
    "server:dev": "ts-node-dev --no-notify --respawn --transpile-only ./src/server",
    "schema:generate": "prisma generate && ts-node --transpile-only ./src/database",
    "migrate:commit": "prisma migrate dev --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:push": "prisma migrate deploy --preview-feature",
    "migrate:validate": "prisma introspect",
    "database": "prisma studio",
    "format": "prettier --write .",
    "lint": "eslint --fix --ext .ts,.tsx,.js .",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@material-ui/core": "^4.11.2",
    "@material-ui/data-grid": "^4.0.0-alpha.13",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@prisma/client": "^2.13.1",
    "@reduxjs/toolkit": "^1.5.0",
    "apollo-boost": "^0.4.9",
    "apollo-server-express": "^2.19.1",
    "bcryptjs": "^2.4.3",
    "chess.js": "^0.11.0",
    "clsx": "^1.1.1",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "graphql-relay": "^0.6.0",
    "immutability-helper": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "microframework": "^0.6.4",
    "module-alias": "^2.2.2",
    "next": "^10.0.3",
    "next-with-apollo": "^5.1.0",
    "nexus": "1.0.0",
    "nexus-plugin-prisma": "^0.27.0",
    "nodemailer": "^6.4.17",
    "nodemon": "^2.0.6",
    "nprogress": "^0.2.0",
    "pretty-ms": "^7.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-resize-detector": "^5.2.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@prisma/cli": "^2.13.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/chess.js": "^0.10.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/graphql-relay": "^0.6.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.165",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.16",
    "@types/nodemailer": "^6.4.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.12",
    "@types/react-resize-detector": "^5.0.0",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-import": "^1.13.3",
    "cypress": "^6.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "prettier": "^2.2.1",
    "ts-babel-node": "^1.1.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma"
  }
}
